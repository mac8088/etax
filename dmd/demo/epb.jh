/**
* The PollutionType (污染类型) Enumerations.
*/
enum PollutionType {
    重金属,
	挥发性有机物,
	半挥发性有机物,
	重金属有机复合污染,
	其他
}

/**
* The PollutionCriterion (污染判别标准) Enumerations.
*/
enum PollutionCriterion {
    筛选值,
	其他标准
}

/**
* The LandDetailedSurvey (场地详细调查) entity.
*/
entity LandDetailedSurvey(epb_land_detailed_survey){
	填报单位
	填写人
	联系电话
	邮箱
一 地块基本情况
	地块名称 (场地名称)
	所在位置
