	/**13.计量单位*/
	measureUnit String required,
    /**14.储存年代*/
    storedtAge String,
    /**15.泄漏或污染痕迹*/
    pollutingFlag Boolean,
    /**16.污染痕迹照片*/
    pollutingPic ImageBlob,
    /**17.是否拆除*/
	dismantledFlag,
    /**18.设施内残余物是否清除*/
	cleanResidueFlag,
    /**19.防护措施*/
