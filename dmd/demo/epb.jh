/**
* The OneToOne relationship.
*/
relationship OneToOne {
    LandUser{industryCategory} to IndustryCategory,
    LandUser{registrationType} to RegistrationType,
	LandInitialSurvey{landParcel} to landParcel,
	LandDetailedSurvey{landParcel} to landParcel,
	LandInitialSurvey{surveyAgency} to ThirdPartyAgency,
	LandInitialSurvey{workingArea} to SurveyWorkingArea
}

/**
* The ManyToOne relationship.
*/
relationship ManyToOne {
	LandParcel{landUser} to LandUser,
    LandUseHistory{landParcel} to LandParcel,
    SurveyWorkingMaterial{workingArea} to SurveyWorkingArea,
    SurveyWorkingProduct{workingArea} to SurveyWorkingArea,
    SurveyWorkingShop{workingArea} to SurveyWorkingArea
}

// Set pagination options
paginate IndustryCategory,RegistrationType with pager
paginate LandOwner with pagination
paginate LandParcel,LandUseHistory with infinite-scroll

